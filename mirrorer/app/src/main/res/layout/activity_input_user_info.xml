<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_base_title_bg_color"
    android:orientation="vertical">
    <!--<LinearLayout-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_marginLeft="55dp"-->
        <!--android:layout_marginRight="55dp"-->
        <!--android:layout_marginTop="70dp"-->
        <!--android:orientation="vertical">-->

        <!--<RelativeLayout-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="45dp"-->
            <!--android:background="#f5f5f5">-->
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/iv_upload_head"
        android:layout_width="80dp"
        android:layout_height="78dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="57dp"
        android:src="@drawable/btnxiangji" />
    <TextView
        android:layout_below="@+id/iv_upload_head"
        android:id="@+id/tv_upload_head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="5dp"
        android:textSize="12sp"
        android:text="上传头像"/>
    <RelativeLayout
        android:layout_below="@+id/tv_upload_head"
        android:id="@+id/ll_choose_sex"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_centerHorizontal="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="102dp"
            android:padding="5dp"
            android:id="@+id/iv_man"
            android:layout_width="wrap_content"
            android:src="@drawable/btn_nanshi_se"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_centerHorizontal="true"
            android:id="@+id/iv_women"
            android:layout_width="wrap_content"
            android:layout_marginRight="100dp"
            android:padding="5dp"
            android:layout_alignParentRight="true"
            android:src="@drawable/btn_woman"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <EditText
        android:id="@+id/et_nick"
        android:layout_width="192dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_choose_sex"
        android:layout_marginTop="35dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:hint="昵称"
        android:textSize="13sp"
        android:textColor="@color/app_dark_black"
        android:textColorHint="#d1d1d1" />
    <TextView
        android:layout_width="192dp"
        android:layout_marginTop="6dp"
        android:layout_centerHorizontal="true"
        android:background="@color/app_gray_color"
        android:layout_below="@+id/et_nick"
        android:layout_height="0.5dp" />
    <!--<TextView-->
        <!--android:id="@+id/register_volid_tv"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:layout_centerVertical="true"-->
        <!--android:gravity="center"-->
        <!--android:text="@string/volid_code_get_text"-->
        <!--android:textSize="14dp" />-->

    <EditText
        android:id="@+id/et_company"
        android:layout_width="192dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_nick"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:hint="公司"
        android:textSize="13sp"
        android:textColor="@color/app_dark_black"
        android:textColorHint="#d1d1d1" />
    <TextView
        android:layout_width="192dp"
        android:layout_marginTop="6dp"
        android:layout_centerHorizontal="true"
        android:background="@color/app_gray_color"
        android:layout_below="@+id/et_company"
        android:layout_height="0.5dp" />
    <EditText
        android:id="@+id/et_position"
        android:layout_width="192dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_company"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:hint="职位"
        android:textSize="13sp"
        android:textColor="@color/app_dark_black"
        android:textColorHint="#d1d1d1" />
    <TextView
        android:layout_width="192dp"
        android:layout_marginTop="6dp"
        android:layout_centerHorizontal="true"
        android:background="@color/app_gray_color"
        android:layout_below="@+id/et_position"
        android:layout_height="0.5dp" />


    <TextView
        android:layout_below="@+id/et_position"
        android:layout_marginTop="31dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/tv_regist_done"
        android:layout_width="192dp"
        android:layout_height="wrap_content"
        android:text="完成"
        android:textColor="#ffffff"
        android:gravity="center"
        android:background="@drawable/login_btn"
        />


    <!--</LinearLayout>-->

</RelativeLayout>